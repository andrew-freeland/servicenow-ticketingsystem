{
  "name": "servicenow-docsdesk",
  "version": "1.0.0",
  "description": "Docs & Integrations Help Desk - ServiceNow integration demo",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "tsx src/seed/seed-docsdesk.ts",
    "test": "NODE_ENV=test tsx --test",
    "test:watch": "NODE_ENV=test tsx --test --watch",
    "tests": "pnpm test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "servicenow",
    "help-desk",
    "integration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "supertest": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
