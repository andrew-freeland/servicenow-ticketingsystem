# ServiceNow PDI URL, e.g. https://dev12345.service-now.com
SERVICE_NOW_INSTANCE=

# Least-privilege API user in PDI
SERVICE_NOW_USER=
SERVICE_NOW_PASSWORD=

# Optional auth (unused unless toggled)
